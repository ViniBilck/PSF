import argparse
from psf.psf import PSF

def main(file_name, stars_coordinates):
   text = PSF(file_name, stars_coordinates)
   coord = (1510,998)
   text.remove_saturated_objects(galaxy_coord=coord, pixel_size=300,
                                 condition_saturation=30, exclusion_radius=15,
                                 plot=True)


if __name__ == '__main__':
    parser = argparse.ArgumentParser()
    parser.add_argument("file_name", help="Data file.")
    parser.add_argument("stars_coordinates", help="Data file.")

    args = parser.parse_args()
    main(args.file_name, args.stars_coordinates)